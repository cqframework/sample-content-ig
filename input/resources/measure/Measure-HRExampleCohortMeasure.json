        "url": "name",
        "valueString": "SDE Sex"
      }, {
        "url": "statement",
        "valueString": "define \"SDE Sex\":\n  case\n    when Patient.sex = '184115007' then \"Patient sex unknown\"\n    when Patient.sex = '248152002' then \"Female\"\n    when Patient.sex = '248153007' then \"Male\"\n    when Patient.sex = 'asked-declined' then \"Asked But Declined\"\n    when Patient.gender = 'male' then \"M\"\n    when Patient.gender = 'female' then \"F\"\n    else null\n  end"
      }, {
        "url": "displaySequence",
        "valueInteger": 0
      } ]
