      }, {
        "url": "name",
        "valueString": "Measure Population"
      }, {
        "url": "statement",
        "valueString": "define \"Measure Population\":\n  \"Initial Population\""
      }, {
        "url": "displaySequence",
        "valueInteger": 11
      } ]
    }, {
      "url": "http://hl7.org/fhir/StructureDefinition/cqf-logicDefinition",
      "extension": [ {
